parameters:
    level: 8
    paths:
        - src/

    checkMissingIterableValueType: false

    bootstrapFiles:
        - '%rootDir%/../../../phpstan-bootstrap.php'

    ignoreErrors:
        - '#Parameter \#2 \$callback of function array_filter expects .+, .+glob.+ given#'

    reportUnmatchedIgnoredErrors: false
